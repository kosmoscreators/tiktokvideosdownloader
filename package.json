{
    "name": "tiktok-scraper",
    "version": "1.4.35",
    "description": "TikTok Scraper & Downloader. Scrape information from User, Trending and HashTag pages and download video posts",
    "main": "./build/index.js",
    "types": "./build/index.d.ts",
    "bin": {
        "tiktok-scraper": "bin/cli.js"
    },
    "files": [
        "build"
    ],
    "dependencies": {
        "archiver": "^7.0.1",
        "async": "^3.2.6",
        "bluebird": "^3.7.2",
        "canvas": "^3.2.0",
        "jsdom": "^24.1.3",
        "json2csv": "^5.0.7",
        "ora": "^5.4.1",
        "progress": "^2.0.3",
        "request": "^2.88.2",
        "request-promise": "^4.2.6",
        "socks-proxy-agent": "^8.0.4",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@types/archiver": "^6.0.2",
        "@types/bluebird": "^3.5.42",
        "@types/jest": "^29.5.13",
        "@types/jsdom": "^21.1.7",
        "@types/json2csv": "^5.0.7",
        "@types/progress": "^2.0.7",
        "@types/request": "^2.48.12",
        "@types/request-promise": "^4.1.51",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.57.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "jest": "^29.7.0",
        "jest-canvas-mock": "^2.5.2",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3"
    },
    "scripts": {
        "build": "rimraf build && tsc",
        "docker:build": "tsc",
        "format": "prettier --config ./.prettierrc.js --write './src/**/*.ts'",
        "lint": "eslint ./src/**/*.ts",
        "test": "eslint ./src/**/*.ts && jest --coverage --verbose"
    },
    "jest": {
        "verbose": true,
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "setupFiles": [
            "jest-canvas-mock"
        ],
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drawrowfly/tiktok-scraper.git"
    },
    "keywords": [
        "TikTok",
        "Scraper"
    ],
    "author": "Andrew Nord <andrew.nord.flow@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/drawrowfly/tiktok-scraper/issues"
    },
    "homepage": "https://github.com/drawrowfly/tiktok-scraper#readme"
}
